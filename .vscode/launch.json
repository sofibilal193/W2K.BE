{
  "version": "0.2.0",
  "configurations": [
  
    {
      "name": "W2K.Mono: Launch",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build services",
      "program": "${workspaceFolder}/src/Services/W2K.Mono/bin/Debug/net10.0/W2K.Mono.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Services/W2K.Mono",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001"
      }
    }
  ]
}
